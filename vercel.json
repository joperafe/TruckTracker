{
  "buildCommand": "npm run build",
  "devCommand": "npm run dev",
  "installCommand": "npm install",
  "framework": "nextjs",
  "functions": {
    "src/app/api/**/*.ts": {
      "maxDuration": 10
    }
  },
  "env": {
    "DATABASE_URI": "@database_uri",
    "AUTH_SECRET": "@auth_secret",
    "AUTH_URL": "@auth_url",
    "PUBLIC_DEFAULT_LAT": "40.7128",
    "PUBLIC_DEFAULT_LNG": "-74.0060"
  }
}